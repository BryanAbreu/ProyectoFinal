@model ViewModels.Viewmodels.CiudadanoViewModel
@{
    ViewData["Title"] = "Editar";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h1>Editar ciudadano</h1>
<form asp-controller="Ciudadano" asp-action="Edit" method="post">
    <div class="form-group">
        <label asp-for="Cedula" class="col-form-label">Cedula</label>
        <input asp-for="Cedula" type="text" class="form-control" id="Nombre">

    </div>
    <div class="form-group">
        <label asp-for="Nombre" class="col-form-label">Nombre</label>
        <input asp-for="Nombre" type="text" class="form-control" id="Apellido">
    </div>
    <div class="form-group">
        <label asp-for="Apellido" class="col-form-label">Apellido</label>
        <input asp-for="Apellido" type="text" class="form-control" id="Partido">
    </div>
    <div class="form-group">
        <label asp-for="Email" class="col-form-label">Email</label>
        <input asp-for="Email" type="text" class="form-control" id="Puesto">
    </div>
    <div class="form-group">
        <label asp-for="Estado" class="col-form-label">Estado</label>
        <select class="custom-select" asp-for="Estado">
            <option value="true">Activo</option>
            <option value="false">Inactivo</option>
        </select>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>



